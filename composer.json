{
    "name":  "aidev/aidev-plugin-starter",
    "type":  "wordpress-plugin",
    "require":  {

                },
    "require-dev":  {
                        "php":  "\u003e=7.4",
                        "dealerdirect/phpcodesniffer-composer-installer":  "^1.0",
                        "squizlabs/php_codesniffer":  "^3.9",
                        "wp-coding-standards/wpcs":  "^3.0",
                        "phpunit/phpunit":  "^9.6",
                        "yoast/phpunit-polyfills":  "^2.0",
                        "phpstan/phpstan":  "^1.11",
                        "szepeviktor/phpstan-wordpress":  "^1.3"
                    },
    "scripts":  {
                    "lint":  "phpcs -q --standard=phpcs.xml aidev-plugin-starter",
                    "lint:fix":  "phpcbf -q --standard=phpcs.xml aidev-plugin-starter",
                    "test":  "phpunit --configuration phpunit.xml.dist",
                    "stan":  "phpstan analyse --no-parallel --memory-limit=1G"
                },
    "config":  {
                   "allow-plugins":  {
                                         "dealerdirect/phpcodesniffer-composer-installer":  true
                                     }
               },
    "description":  "AIDevelopment WordPress plugin starter with CI/CD, tests, PHPCS, PHPStan, and Playwright.",
    "license":  "GPL-2.0-or-later"
}